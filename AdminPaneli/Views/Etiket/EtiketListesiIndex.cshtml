@model IEnumerable<BlogSitesi.Etiketler>

@{
    ViewBag.Title = "EtiketListesiIndex";
    Layout = "~/Areas/AdminPaneli/Views/Shared/AdminLayoutPage.cshtml";
}

<div class="container">
    <div class="col-md- offset-2">
        <h2>EtiketListesiIndex</h2>

        <p>
            @Html.ActionLink("Create New", "Create")
        </p>
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.EtiketAdi)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.EtiketAdi)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.EtiketlerID }) |
                        @Html.ActionLink("Details", "Details", new { id = item.EtiketlerID }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.EtiketlerID })
                    </td>
                </tr>
            }

        </table>
    </div>
</div>