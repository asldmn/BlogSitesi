@model List<Kategoriler>

<div class="col-md-3 integ">
    <div class="l_g_r">
        <div class="categories">
            <h4>Kategoriler</h4>

            @foreach (var item in Model)
            {
                <h6><a href="#">@item.KategoriAdi</a></h6>
            }
        </div>
    </div>
</div>


